{
  "extends": ["next/core-web-vitals", "next/typescript"],
  "rules": {
    "no-unused-vars": "warn",
    "no-undef": "error",
    "no-multi-spaces": "error",
    "object-curly-spacing": [ "error", "always" ],
    "array-bracket-spacing": [ "error", "always" ],
    "key-spacing": [ "error", { "beforeColon": false, "afterColon": true } ]
  }
}
